// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       String     @id @default(uuid())
  email    String  @unique
  name     String?
  password String?
  sensors  Sensor[]
}

model Sensor {
  id       String     @id @default(uuid())
  owner    User @relation(fields: [ownerId], references: [id])
  ownerId  String // relation scalar field  (used in the `@relation` attribute above)
  dev_id   String
  lati     Float
  temp_in  Float
  cond     Float
  nitrate  Float
  potass   Float
  phosph   Float
  temp_out Float
  longi    Float
  humi_in  Float
  ph       Float
  sel      Float
  humi_out Float
  gaz_1    Float
  gaz_2    Float
  gaz_3    Float
  gaz_4    Float
}
